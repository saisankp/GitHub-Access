{
  "name": "visualization",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "visualize": "node ./server/server.js & npm run serve",
    "start": "node ./server/server.js & npm run build && serve -s -n dist -l 8080",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "finish" : "serve -s dist -l 8080",
    "lint": "vue-cli-service lint",
    "test": "jest --coverage --forceExit"
  },
  "jest": {
    "testMatch": [
      "**/tests/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx",
      "node",
      "vue"
    ],
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    }
  },
  "dependencies": {
    "axios": "^0.21.4",
    "core-js": "^3.18.0",
    "cors": "^2.8.5",
    "mongodb": "^4.2.2",
    "node-fetch": "^3.1.0",
    "selenium-webdriver": "^3.6.0",
    "serve": "^13.0.2",
    "vue": "^2.6.10",
    "vue-resource": "^1.5.3",
    "vue-router": "^3.5.2",
    "vuetify": "^2.4.9"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.13.13",
    "@vue/babel-preset-app": "^4.5.12",
    "@vue/cli-plugin-babel": "^3.3.0",
    "@vue/cli-plugin-eslint": "^3.1.1",
    "@vue/cli-service": "^3.12.1",
    "@vue/test-utils": "^1.1.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^26.6.3",
    "eslint": "^7.24.0",
    "eslint-plugin-vue": "^7.8.0",
    "jest": "^26.6.3",
    "jest-serializer-vue": "^2.0.2",
    "sass": "^1.32.0",
    "sass-loader": "^10.1.1",
    "supertest": "^6.1.3",
    "vue-cli-plugin-vuetify": "^2.3.1",
    "vue-jest": "^3.0.7",
    "vue-multiselect": "^2.1.6",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.7.0",
    "zingchart": "^2.9.3",
    "zingchart-vue": "^2.0.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
